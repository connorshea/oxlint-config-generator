    "no-styled-jsx-in-document": {
      "recommended": true,
      "fixable": false
    },
    "no-sync-scripts": {
      "recommended": true,
      "fixable": false
    },
    "no-title-in-document-head": {
      "recommended": true,
      "fixable": false
    },
    "no-typos": {
      "recommended": true,
      "fixable": false
    },
    "no-unwanted-polyfillio": {
      "recommended": true,
      "fixable": false
    }
