{
  "name": "next",
  "rules": {
    "google-font-display": {
      "recommended": true,
      "fixable": false
    },
    "google-font-preconnect": {
      "recommended": true,
      "fixable": false
    },
    "inline-script-id": {
      "recommended": true,
      "fixable": false
    },
    "next-script-for-ga": {
      "recommended": true,
      "fixable": false
    },
    "no-assign-module-variable": {
      "recommended": true,
      "fixable": false
    },
    "no-async-client-component": {
      "recommended": true,
      "fixable": false
    },
    "no-before-interactive-script-outside-document": {
      "recommended": true,
      "fixable": false
    },
    "no-css-tags": {
      "recommended": true,
      "fixable": false
    },
    "no-document-import-in-page": {
      "recommended": true,
      "fixable": false
    },
    "no-duplicate-head": {
      "recommended": true,
      "fixable": false
    },
    "no-head-element": {
      "recommended": true,
      "fixable": false
