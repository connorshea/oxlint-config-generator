{
  "name": "jsx-a11y",
  "rules": {
    "accessible-emoji": {
      "recommended": false,
      "fixable": false
    },
    "alt-text": {
      "recommended": false,
      "fixable": false
    },
    "anchor-ambiguous-text": {
      "recommended": false,
      "fixable": false
    },
    "anchor-has-content": {
      "recommended": false,
      "fixable": false
    },
    "anchor-is-valid": {
      "recommended": false,
      "fixable": false
    },
    "aria-activedescendant-has-tabindex": {
      "recommended": false,
      "fixable": false
    },
    "aria-props": {
      "recommended": false,
      "fixable": false
    },
    "aria-proptypes": {
      "recommended": false,
      "fixable": false
    },
    "aria-role": {
      "recommended": false,
      "fixable": false
    },
    "aria-unsupported-elements": {
      "recommended": false,
      "fixable": false
    },
    "autocomplete-valid": {
      "recommended": false,
      "fixable": false
    },
    "click-events-have-key-events": {
      "recommended": false,
      "fixable": false
    },
    "control-has-associated-label": {
      "recommended": false,
      "fixable": false
    },
    "heading-has-content": {
      "recommended": false,
      "fixable": false
    },
    "html-has-lang": {
      "recommended": false,
      "fixable": false
    },
    "iframe-has-title": {
      "recommended": false,
      "fixable": false
    },
    "img-redundant-alt": {
      "recommended": false,
      "fixable": false
    },
    "interactive-supports-focus": {
      "recommended": false,
      "fixable": false
    },
    "label-has-associated-control": {
      "recommended": false,
      "fixable": false
    },
    "label-has-for": {
      "recommended": false,
      "fixable": false
    },
    "lang": {
      "recommended": false,
      "fixable": false
    },
    "media-has-caption": {
      "recommended": false,
      "fixable": false
    },
    "mouse-events-have-key-events": {
      "recommended": false,
      "fixable": false
    },
    "no-access-key": {
      "recommended": false,
      "fixable": false
    },
    "no-aria-hidden-on-focusable": {
      "recommended": false,
      "fixable": false
    },
    "no-autofocus": {
      "recommended": false,
      "fixable": false
    },
    "no-distracting-elements": {
      "recommended": false,
      "fixable": false
    },
    "no-interactive-element-to-noninteractive-role": {
      "recommended": false,
      "fixable": false
    },
    "no-noninteractive-element-interactions": {
      "recommended": false,
      "fixable": false
    },
    "no-noninteractive-element-to-interactive-role": {
      "recommended": false,
      "fixable": false
    },
    "no-noninteractive-tabindex": {
      "recommended": false,
      "fixable": false
    },
    "no-onchange": {
      "recommended": false,
      "fixable": false
    },
    "no-redundant-roles": {
      "recommended": false,
      "fixable": false
    },
    "no-static-element-interactions": {
      "recommended": false,
      "fixable": false
    },
    "prefer-tag-over-role": {
      "recommended": false,
      "fixable": false
    },
    "role-has-required-aria-props": {
      "recommended": false,
      "fixable": false
    },
    "role-supports-aria-props": {
      "recommended": false,
      "fixable": false
    },
    "scope": {
      "recommended": false,
      "fixable": false
    },
    "tabindex-no-positive": {
      "recommended": false,
      "fixable": false
    }
  },
  "recommendedConfig": {
    "jsx-a11y/alt-text": "error",
    "jsx-a11y/anchor-ambiguous-text": "off",
    "jsx-a11y/anchor-has-content": "error",
    "jsx-a11y/anchor-is-valid": "error",
    "jsx-a11y/aria-activedescendant-has-tabindex": "error",
    "jsx-a11y/aria-props": "error",
    "jsx-a11y/aria-proptypes": "error",
    "jsx-a11y/aria-role": "error",
    "jsx-a11y/aria-unsupported-elements": "error",
    "jsx-a11y/autocomplete-valid": "error",
    "jsx-a11y/click-events-have-key-events": "error",
    "jsx-a11y/control-has-associated-label": [
      "off",
      {
        "ignoreElements": [
          "audio",
          "canvas",
          "embed",
          "input",
          "textarea",
          "tr",
          "video"
        ],
        "ignoreRoles": [
          "grid",
          "listbox",
          "menu",
          "menubar",
          "radiogroup",
          "row",
          "tablist",
          "toolbar",
          "tree",
          "treegrid"
        ],
        "includeRoles": [
          "alert",
          "dialog"
        ]
      }
    ],
    "jsx-a11y/heading-has-content": "error",
    "jsx-a11y/html-has-lang": "error",
    "jsx-a11y/iframe-has-title": "error",
    "jsx-a11y/img-redundant-alt": "error",
    "jsx-a11y/interactive-supports-focus": [
      "error",
      {
        "tabbable": [
          "button",
          "checkbox",
          "link",
          "searchbox",
          "spinbutton",
          "switch",
          "textbox"
        ]
      }
    ],
    "jsx-a11y/label-has-associated-control": "error",
    "jsx-a11y/label-has-for": "off",
    "jsx-a11y/media-has-caption": "error",
    "jsx-a11y/mouse-events-have-key-events": "error",
    "jsx-a11y/no-access-key": "error",
    "jsx-a11y/no-autofocus": "error",
    "jsx-a11y/no-distracting-elements": "error",
    "jsx-a11y/no-interactive-element-to-noninteractive-role": [
      "error",
      {
        "tr": [
          "none",
          "presentation"
        ],
        "canvas": [
          "img"
        ]
      }
    ],
    "jsx-a11y/no-noninteractive-element-interactions": [
      "error",
      {
        "handlers": [
          "onClick",
          "onError",
          "onLoad",
          "onMouseDown",
          "onMouseUp",
          "onKeyPress",
          "onKeyDown",
          "onKeyUp"
        ],
        "alert": [
          "onKeyUp",
          "onKeyDown",
          "onKeyPress"
        ],
        "body": [
          "onError",
          "onLoad"
        ],
        "dialog": [
          "onKeyUp",
          "onKeyDown",
          "onKeyPress"
        ],
        "iframe": [
          "onError",
          "onLoad"
        ],
        "img": [
          "onError",
          "onLoad"
        ]
      }
    ],
    "jsx-a11y/no-noninteractive-element-to-interactive-role": [
      "error",
      {
        "ul": [
          "listbox",
          "menu",
          "menubar",
          "radiogroup",
          "tablist",
          "tree",
          "treegrid"
        ],
        "ol": [
          "listbox",
          "menu",
          "menubar",
          "radiogroup",
          "tablist",
          "tree",
          "treegrid"
        ],
        "li": [
          "menuitem",
          "menuitemradio",
          "menuitemcheckbox",
          "option",
          "row",
          "tab",
          "treeitem"
        ],
        "table": [
          "grid"
        ],
        "td": [
          "gridcell"
        ],
        "fieldset": [
          "radiogroup",
          "presentation"
        ]
      }
    ],
    "jsx-a11y/no-noninteractive-tabindex": [
      "error",
      {
        "tags": [],
        "roles": [
          "tabpanel"
        ],
        "allowExpressionValues": true
      }
    ],
    "jsx-a11y/no-redundant-roles": "error",
    "jsx-a11y/no-static-element-interactions": [
      "error",
      {
        "allowExpressionValues": true,
        "handlers": [
          "onClick",
          "onMouseDown",
          "onMouseUp",
          "onKeyPress",
          "onKeyDown",
          "onKeyUp"
        ]
      }
    ],
    "jsx-a11y/role-has-required-aria-props": "error",
    "jsx-a11y/role-supports-aria-props": "error",
    "jsx-a11y/scope": "error",
    "jsx-a11y/tabindex-no-positive": "error"
  }
}