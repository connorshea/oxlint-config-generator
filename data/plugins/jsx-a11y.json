      "deprecated": false
    },
    "click-events-have-key-events": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "control-has-associated-label": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "heading-has-content": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "html-has-lang": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "iframe-has-title": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "img-redundant-alt": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "interactive-supports-focus": {
      "recommended": true,
      "fixable": false,
      "deprecated": false
    },
    "label-has-associated-control": {
      "recommended": true,
