{
  "name": "oxlint-config-generator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "oxlint",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "generate:rules": "tsx scripts/generate-rules.ts",
    "generate:plugins": "tsx scripts/generate-plugins.ts",
    "generate:rule-descriptions": "tsx scripts/generate-rule-descriptions.ts",
    "generate:all": "pnpm generate:rules && pnpm generate:plugins && pnpm generate:rule-descriptions && pnpm fmt"
  },
  "dependencies": {
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.4",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jsdoc": "^62.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^10.7.0",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.3"
  },
  "lint-staged": {
    "*": "oxfmt --no-error-on-unmatched-pattern"
  }
}
